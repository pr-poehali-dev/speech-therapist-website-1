      "method": "GET",
      "path": "/?id=articulation",
      "expectedStatus": 200,
      "expectedBody": {
        "id": "articulation",
        "name": "string",
        "filename": "string",
        "download_url": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Handle OPTIONS for CORS",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200
    },
    {
